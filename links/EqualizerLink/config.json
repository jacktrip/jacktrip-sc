{
    "label": "Equalizer",
    "class": "EqualizerLink",
    "options": [
        {
            "src": "freq",
            "label": "Frequency (Hz)",
            "type": "number",
            "defaultVal": 300,
            "field": {
                "type": "slider",
                "fieldOptions": {
                    "marks": [
                        {
                            "value": 100,
                            "label": "100Hz"
                        },
                        {
                            "value": 500,
                            "label": "500Hz"
                        },
                        {
                            "value": 2500,
                            "label": "2.5kHz"
                        }
                    ],
                    "logarithmic": true,
                    "min": 20,
                    "max": 20000,
                    "step": 1
                }
            }
        },
        {
            "src": "gain",
            "label": "Gain (dB)",
            "type": "number",
            "defaultVal": 0,
            "field": {
                "type": "slider",
                "fieldOptions": {
                    "marks": [
                        {
                            "value": -12,
                            "label": "-12dB"
                        },
                        {
                            "value": 0,
                            "label": "0dB"
                        },
                        {
                            "value": 12,
                            "label": "+12dB"
                        }
                    ],
                    "logarithmic": false,
                    "min": -30,
                    "max": 30,
                    "step": 0.100
                }
            }
        },
        {
            "src": "q",
            "label": "Q Shape",
            "type": "number",
            "defaultVal": 1,
            "field": {
                "type": "slider",
                "fieldOptions": {
                    "marks": [
                        {
                            "value": 0.2,
                            "label": "Narrower"
                        },
                        {
                            "value": 1,
                            "label": "1"
                        },
                        {
                            "value": 7,
                            "label": "Wider"
                        }
                    ],
                    "logarithmic": true,
                    "min": 0.1,
                    "max": 10
                }
            }
        }
    ]
}
